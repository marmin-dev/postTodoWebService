{{>layout/header}}
<h1 style="text-align:center "><a href="/" style="text-decoration:none,color:black">TodoPostWeb</a></h1>
<nav class="navbar navbar-expand-lg bg-light">
    <div class="container px-4">
        <a class="navbar-brand" href="/">내게시물</a>
        <a class="navbar-brand" href="/post/all">전체게시물</a>
        <a class="navbar-brand" href="/post/create">게시물작성</a>
        <a class="navbar-brand" href="/">할일추가</a>
    </div>
</nav>
{{#post}}
    <div class="card">
        <div class="card-body">
            <h5 class="card-title"><a href="/post/find/{{id}}">{{title}}</a></h5>
            <h6 class="class=card-subtitle mb-2 text-muted">{{author}}</h6>
            <p class="card-text">{{content}}</p>
        </div>
    </div>
{{/post}}
{{>layout/footer}}
